# set up the working directory
set work work
vlib work

# compile all of the files
vcom -work work ../../implement/results/routed.vhd
vcom -work work dotclock100_tb.vhd

# run the simulation
vsim -t ps +transport_int_delays -voptargs="+acc" -L secureip -L simprim -sdfmax dotclock100_tb/dut=../../implement/results/routed.sdf +no_notifier work.dotclock100_tb 
when {end_of_sim} {echo "End of Simulation"; stop} 
#do wave.do
#log -r /*
run 50000ns


